<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <style>
    html,
    body {
      border: 0;
      margin: 0;
      padding: 0;
      font: 14px "Lato", Arial, sans-serif;
      min-width: 100%;
      min-height: 100%;
    }

    .hidden {
      content: '';
      visibility: hidden;
      height: 0;
      display: contents;
    }

    /* To DoList */
    .todolist {
      display: flex;
      position: absolute;
      width: 100%;
      height: 100%;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      background-color: #f9f7f4;
    }

    .list_item {
      display: flex;
      align-items: center;
      justify-content: space-between;
      margin: 1rem 0px;
      padding: 0.5rem;
      border-color: #666666;
      border-radius: 5px;
      background-color: #fff;
      transition: 0.5s;
      box-shadow: #666666 1px 10px 15px -10px;
    }

    .list_item .list_item-name {
      font-weight: 700;
      color: #00aad4;
    }

    .complete.list_item .list_item-name {
      color: #666666;
      text-decoration: line-through 1px;
    }

    .disable {
      opacity: 0.6;
      cursor: not-allowed;
    }

    .head {
      display: flex;
      width: 100%;
      position: relative;
      align-items: center;
      justify-content: space-between;
    }

    .head h1 {
      text-align: left;
    }

    .head p {
      text-align: right;
    }

    .search {
      display: flex;
      justify-content: space-between;
    }

    .task {
      display: flex;
      justify-content: space-between;
    }

    /* End to do list */
    .none {
      display: none;
    }
  </style>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
