<%- include('../includes/head.ejs') %>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css"
    integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  <style>
    table{
      margin-top: 5rem;
    }
  </style>
  <%- include('../includes/nav.ejs') %>
    </header>

    <body>
      <div class="container">
        <table class="table table-hover">
          <thead class="thead thead-dark" style="background-color: #acb2c7">
            <tr>
              <th scope="col">#</th>
              <th scope="col">Member</th>
              <th scope="col">Classroom's Name</th>
              <th scope="col">Created At</th>
              <th scope="col">Updated At</th>
              <!-- <th scope="col">&nbsp;</th>
        <th scope="col">&nbsp;</th>
        <th scope="col">&nbsp;</th> -->
            </tr>
          </thead>
          <tbody>
            <% for(let classroom of classrooms){ %>
              <tr>
                <th scope="row">
                  <%= classroom.id %>
                </th>
                <td>
                  <%= classroom.account_class.quantity %>
                </td>
                <td>
                  <a href="/teacher/classroom/<%= classroom.id %>">
                    <%= classroom.name %>
                  </a>
                </td>
                <td>
                  <%= classroom.createdAt.toUTCString() %>
                </td>
                <td>
                  <%= classroom.updatedAt.toUTCString() %>
                </td>
              </tr>
              <% } %>
          </tbody>
        </table>
      </div>
    </body>

    </html>